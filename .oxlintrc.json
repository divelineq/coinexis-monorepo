{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "rules": {
    // Не нужно из-за typescript
    "eslint/constructor-super": "off",
    "eslint/no-class-assign": "off",
    "eslint/no-dupe-class-members": "off",
    "eslint/no-dupe-keys": "off",
    "eslint/no-func-assign": "off",
    "eslint/no-new-native-nonconstructor": "off",
    "eslint/no-obj-calls": "off",
    "eslint/no-setter-return": "off",
    "eslint/no-this-before-super": "off",
    "eslint/no-unsafe-negation": "off",
    "oxc/bad-array-method-on-arguments": "off",

    "eslint/no-unsafe-optional-chaining": [
      "warn",
      { "disallowArithmeticOperators": true }
    ],
    "eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true,
        "reportVarsOnlyUsedAsTypes": true
      }
    ],
    "eslint/default-case-last": "warn",
    "eslint/eqeqeq": ["warn", "always", { "null": "ignore" }],
    "eslint/no-constant-binary-expression": "warn",
    "eslint/no-eslint-return": "warn",
    "eslint/no-empty": "warn",
    "eslint/no-empty-function": "off",
    "eslint/no-promise-executor-return": "warn",
    "eslint/no-unreachable": "warn",
    "eslint/no-unused-private-class-members": "warn",
    "eslint/no-useless-rename": "warn",
    "eslint/no-useless-constructor": "warn",
    "eslint/no-lonely-if": "warn",
    "eslint/max-classes-per-file": [
      "error",
      { "ignoreExpressions": true, "max": 1 }
    ],
    "eslint/no-array-constructor": "error",
    "eslint/max-depth": "error",
    "eslint/no-alert": "error",
    "eslint/no-await-in-loop": "error",
    "eslint/no-case-declarations": "error",
    "eslint/no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "eslint/no-constructor-return": "error",
    "eslint/no-fallthrough": "error",
    "eslint/no-prototype-builtins": "error",
    "eslint/no-regex-spaces": "error",
    "eslint/no-self-compare": "error",
    "eslint/no-unexpected-multiline": "error",
    "eslint/no-var": "error",
    "eslint/prefer-rest-params": "error",
    "eslint/prefer-spread": "error",
    "unicorn/consistent-function-scoping": "error",
    "unicorn/error-message": "error",
    "unicorn/new-for-builtins": "error",
    "unicorn/no-abusive-eslint-disable": "off", //! ошибка выжившего
    "unicorn/no-document-cookie": "error",
    "unicorn/no-object-as-default-parameter": "error",
    "unicorn/no-this-assignment": "error",
    "unicorn/no-typeof-undefined": "error",
    "unicorn/no-unreadable-iife": "error",
    "unicorn/prefer-math-trunc": "error",
    "unicorn/prefer-math-min-max": "warn",
    "unicorn/catch-error-name": "warn",
    "unicorn/escape-case": "warn",
    "unicorn/explicit-length-check": "warn",
    "unicorn/no-array-for-each": "warn",
    "unicorn/no-await-expression-member": "warn",
    "typescript/prefer-for-of": "warn",
    "unicorn/no-hex-escape": "warn",
    "unicorn/no-instanceof-builtins": "warn",
    "unicorn/no-new-buffer": "warn",
    "unicorn/no-static-only-class": "warn",
    "unicorn/no-useless-error-capture-stack-trace": "warn",
    "unicorn/numeric-separators-style": "warn",
    "unicorn/prefer-add-event-listener": "off",
    "unicorn/prefer-array-flat": "warn",
    "unicorn/prefer-array-some": "warn",
    "unicorn/prefer-array-index-of": "warn",
    "unicorn/prefer-array-find": "warn",
    "unicorn/prefer-array-flat-map": "warn",
    "unicorn/prefer-at": "warn",
    "unicorn/prefer-date-now": "warn",
    "unicorn/prefer-default-parameters": "warn",
    "unicorn/prefer-dom-node-append": "warn",
    "unicorn/prefer-dom-node-dataset": "warn",
    "unicorn/prefer-dom-node-remove": "warn",
    "unicorn/prefer-object-from-entries": "warn",
    "unicorn/prefer-includes": "warn",
    "unicorn/prefer-keyboard-event-key": "warn",
    "unicorn/prefer-logical-operator-over-ternary": "warn",
    "unicorn/prefer-modern-dom-apis": "warn",
    "unicorn/prefer-modern-math-apis": "warn",
    "unicorn/prefer-native-coercion-functions": "warn",
    "unicorn/prefer-negative-index": "warn",
    "unicorn/prefer-optional-catch-binding": "warn",
    "unicorn/prefer-reflect-apply": "warn",
    "unicorn/prefer-regexp-test": "warn",
    "unicorn/prefer-string-slice": "warn",
    "unicorn/prefer-string-trim-start-end": "warn",
    "unicorn/prefer-type-error": "warn",
    "unicorn/require-number-to-fixed-digits-argument": "warn",
    "unicorn/require-array-join-separator": "warn",
    "unicorn/throw-new-error": "warn",
    "import/export": "error",
    "react/button-has-type": "error",
    "react/jsx-no-script-url": "error",
    "react/void-dom-elements-no-children": "error",
    "react/jsx-boolean-value": "warn",
    "react/jsx-curly-brace-presence": [
      "warn",
      { "props": "never", "children": "never" }
    ],
    "react/jsx-no-useless-fragment": "warn",
    "react/jsx-pascal-case": "warn",
    "react/self-closing-comp": "warn",
    "react/exhaustive-deps": "warn",
    "react/rules-of-hooks": "error",
    "typescript/adjacent-overload-signatures": "error",
    "typescript/ban-ts-comment": ["error", { "allow-with-description": true }],
    "typescript/no-empty-object-type": [
      "error",
      { "allowInterfaces": "with-single-extends" }
    ],
    "typescript/no-inferrable-types": "error",
    "typescript/no-namespace": "error",
    "typescript/no-unnecessary-type-constraint": "error",
    "typescript/no-unsafe-function-type": "error",
    "typescript/prefer-literal-enum-member": "error",
    "typescript/prefer-namespace-keyword": "error",
    "eslint/require-await": "error",
    "typescript/array-type": "warn",
    "typescript/no-confusing-non-null-assertion": "warn"
  }
}
